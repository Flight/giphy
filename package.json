{
  "name": "giphy",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview --open",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "e2e": "start-server-and-test 'pnpm build && vite preview' http://localhost:4173/ 'cypress open --e2e'",
    "e2e:silent": "start-server-and-test 'pnpm build && vite preview' http://localhost:4173/ 'cypress run'",
    "pre-commit": "tsc && eslint --config .eslintrc.cjs --max-warnings=0 && prettier ./src --write && pnpm coverage",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./storybook",
    "preview-storybook": "vite preview --outDir ./storybook --open"
  },
  "dependencies": {
    "daisyui": "^2.43.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5"
  },
  "devDependencies": {
    "@giphy/js-types": "^4.4.0",
    "@storybook/addon-essentials": "^7.0.0-beta.6",
    "@storybook/addon-interactions": "^7.0.0-beta.6",
    "@storybook/addon-links": "^7.0.0-beta.6",
    "@storybook/blocks": "^7.0.0-beta.6",
    "@storybook/react": "^7.0.0-beta.6",
    "@storybook/react-vite": "^7.0.0-beta.6",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.1.0",
    "cypress-vite": "^1.2.1",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.8",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.20",
    "postcss-nested": "^6.0.0",
    "prettier": "^2.8.1",
    "start-server-and-test": "^1.15.2",
    "storybook": "7.0.0-beta.6",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.25.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite"
      ]
    }
  }
}